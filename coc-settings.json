{
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips",
  "languageserver": {
    "dart": {
      "command": "dart",
      "args": ["/usr/local/google/home/jmccandless/Projects/flutter/bin/cache/dart-sdk/bin/snapshots/analysis_server.dart.snapshot", "--lsp", "--client-id coc-flutter"],
      "filetypes": ["dart"],
      "initializationOptions": {},
      "settings": {
        "dart": {
          "validation": {},
          "completion": {}
        }
      }
    },
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "c",
        "cpp",
        "objc",
        "objcpp"
      ],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  }
  // coc-flutter settings
  //"dart.completeFunctionCalls": false
  "flutter.runDevToolsAtStartup": true
}
